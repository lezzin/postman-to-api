# Cabeçalhos que devem ser exibidos/documentados (separados por vírgula)
REQUEST_HEADERS_WHITELIST=Content-Type,Accept,Authorization

# Chaves sensíveis a serem ocultadas (ex: password, token, authorization)
SENSITIVE_KEYS=

# Caminho da pasta contendo os arquivos .json exportados do Postman
POSTMAN_FOLDER="postman"

# Número máximo de respostas a serem exibidas em um JSON
MAX_RESPONSES=2

# Tamanho máximo do corpo de um JSON para exibição (em caracteres)
MAX_JSON_LENGTH=5000

# Tamanho máximo permitido para uma chave JSON
MAX_KEY_LENGTH=1000

# Número máximo de itens em arrays que serão exibidos em um JSON
MAX_ARRAY_ITEMS=10
